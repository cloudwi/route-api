# This file shows example environment variables
# The actual credentials should be stored in config/credentials.yml.enc

# To edit credentials, run:
# EDITOR="vim" rails credentials:edit

# Add the following to your credentials.yml.enc:
# kakao:
#   client_id: your_kakao_client_id
#   client_secret: your_kakao_client_secret

# Secret key base is automatically managed by Rails credentials
# You can also add custom JWT secret if needed:
# jwt_secret: your_custom_jwt_secret_key
